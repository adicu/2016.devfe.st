{{#extend "base"}}
{{#content "main" mode="prepend"}}

<section id="top" class="page page-landing">
  <div class="content">
    <div class="grid-container">
      <div class="row">
        <div class="land-row col-5">
          <h1 class="intro-land-1">
            7 days, 6 tracks, 1 hackathon.<br>
            Everything you won't learn in class.<br>
            DevFest 2016 at Columbia University.
          </h1>
          <div class="row intro-row">
            <a id="success" data-mode="1" target="_blank" href="http://goo.gl/forms/aLOscVP2m4" style="" class="scroll typeform-share">
              <div class="register_button col-3 showcase-center">
                DevFest Feedback Form
              </div>
            </a>

          </div>
          <div class="row"><p class="date">Take off January 30th - February 6th, 2016</p></div>
          <div class="row"><p class="date">All events for 2016 have concluded</p></div>
          <div class="row"><p class="date">See you next year!</p></div>



          <div class="more row">
            <div class="col-6 next_page">
              <a class="scroll more" href="#schedule">
                <p class="showcase-center">Learn More</p><br>
                <p  class="showcase-center"><i class="fa fa-angle-down fa-2x"></i></p>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>


  </div>


  <img id="globe" class="half-globe hide-mobile" src="/img/bw-globe.svg">
  <img style="display: none" id="airplane" class="plane hide-mobile" src="/img/plane.png">
</section>


